#include <cstdio>

#define INF 0x3f3f3f3f

static int seq[100];
static int f[100][100];

int dp(int l, int r) {
    if (l == r)
        return -seq[l];
    if (f[l][r] < INF)
        return f[l][r];

    int &ans = f[l][r];
}

int main() {
    int n;
    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {
        scanf("%d", seq + i);
    }

    memset(f, 0x3f, sizeof(f));

    return 0;
}
