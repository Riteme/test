// #define NDEBUG

#include <cassert>
#include <cstdio>
#include <cstring>

#include <vector>
#include <algorithm>

using namespace std;

#define NMAX 100000
#define BASE 27

typedef unsigned long long u64;

inline u64 code(char c) {
    return c - 'a';
}

static vector<char> stk;
static int n;
static char s[NMAX + 10];
static bool right[NMAX + 10];
static u64 suf[NMAX + 10]

void initialize() {
    scanf("%s", s + 1);
    n = strlen(s + 1);
}

int main() {
#ifdef USE_FILE_IO
    freopen("match.in", "r", stdin);
    freopen("match.out", "w", stdout);
#endif
    initialize();

    return 0;
}
