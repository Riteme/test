#include <cstdio>
#include <cstring>

#include <algorithm>
#include <unordered_map>

using namespace std;

using i64 = long long;

constexpr int NMAX = 50;
constexpr i64 TMAX = 100000;

int n;
int a[NMAX];
unordered_map<i64, int> cnt;

int main() {
    int T;
    scanf("%d", &T);
    while (T--) {
        scanf("%d", &n);
        for (int i = 0; i < n; i++) {
            scanf("%d", a + i);
        }
        cnt.clear();
        cnt.reserve(TMAX * n);
        for (i64 t = 0; t <= TMAX; t++) {
            for (int i = 0; i < n; i++) {
                cnt[t * t - a[i]]++;
            }
        }

        int ans = 1;
        for (auto &e : cnt) {
            if (0 <= e.first)
                ans = max(ans, e.second);
        }
        printf("%d\n", ans);
    }
    return 0;
}
